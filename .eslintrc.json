// remove unused imports
{
	// "parser": "@typescript-eslint/parser",
	"parserOptions": {
		"project": "./tsconfig.json",
		"sourceType": "module"
	},
	"plugins": ["@typescript-eslint", "import"],
	"rules": {
		"@typescript-eslint/no-unused-imports": "off"
	},

	"extends": [
		"next/core-web-vitals",
		"eslint:recommended",
		"airbnb-typescript/base",
		"prettier"
	]
}
